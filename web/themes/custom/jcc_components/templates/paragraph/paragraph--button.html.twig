{% set custom_css = paragraph.field_custom_classes.value ? ' ' ~ paragraph.field_custom_classes.value : '' %}
{% set link = content.field_link.0 %}

{% include '@atoms/button/button.twig' with {
  button: {
    text: link['#title'],
    url: link['#url']|render ? link['#url']|render : 'javascript:void(0);',
    style: 'primary' ~ custom_css,
  }
} %}
