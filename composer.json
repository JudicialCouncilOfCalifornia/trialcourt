{
    "name": "chapter-three/jcc-srl",
    "description": "Judicial Council of California Self-Represented Litigant portal - Drupal 8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "ckeditor/templates": {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/templates",
                "version": "4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.13.1.zip",
                    "type": "zip"
                }
            }
        },
        "enyo/dropzone": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.5.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "v5.5.0"
                }
            }
        },
        "components/highlightjs": {
            "type": "package",
            "package": {
                "name": "components/highlightjs",
                "version": "9.7.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
                    "type": "zip"
                }
            }
        },
        "d3/d3": {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
                    "type": "zip"
                }
            }
        },
        "harvesthq/chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    },
    "require": {
        "php": ">=7.3",
        "ckeditor-plugin/templates": "^4.13",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/allowed_formats": "^1.1",
        "drupal/autosave_form": "^1.0",
        "drupal/chosen": "^2.8",
        "drupal/components": "^1.1",
        "drupal/config_exclude": "1.x-dev",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/core": "^8.7.0",
        "drupal/diff": "^1.0@RC",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_embed": "^1.0@RC",
        "drupal/environment_indicator": "^3.6",
        "drupal/extlink": "^1.1",
        "drupal/field_group": "^3.0-rc1",
        "drupal/focal_point": "^1.0",
        "drupal/geocoder": "^3.0",
        "drupal/geolocation": "^3.0",
        "drupal/google_tag": "^1.2",
        "drupal/honeypot": "^1.29",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/key": "^1.10",
        "drupal/linkit": "^4.3",
        "drupal/login_emailusername": "^1.1",
        "drupal/media_entity_browser": "^2.0",
        "drupal/metatag": "^1.8",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_source_csv": "^3.2",
        "drupal/migrate_tools": "^4.5",
        "drupal/module_filter": "^3.1",
        "drupal/office_hours": "^1.2",
        "drupal/pathauto": "^1.4",
        "drupal/r4032login": "^1.1",
        "drupal/range": "^1.1",
        "drupal/redirect": "^1.3",
        "drupal/role_delegation": "^1.0",
        "drupal/scheduler": "^1.1",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/search404": "^1.0",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_solr": "^3.4",
        "drupal/smart_trim": "^1.1",
        "drupal/twig_tweak": "^2.4",
        "drupal/webform": "^5.4",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.5",
        "geocoder-php/bing-maps-provider": "^4.0",
        "harvesthq/chosen": "^1.8.7",
        "judicialcouncil/jcc_base": "^1.0.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal/console": "^1.9",
        "drupal/devel": "^2.1",
        "drupal/stage_file_proxy": "^1.0@RC",
        "drupal/twig_xdebug": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf",
            "find .git/ -type d -name hooks | xargs rm -rf",
            "if [ ! -f web/sites/default/services.local.yml ]; then cp examples/example.services.local.yml web/sites/default/services.local.yml; fi",
            "if [ ! -f web/sites/default/settings.local.php ]; then cp examples/example.settings.local.php web/sites/default/settings.local.php; fi"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf",
            "find .git/ -type d -name hooks | xargs rm -rf"
        ],
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff step defined.'",
        "unit-test": "echo 'No unit test step defined.'",
        "artifact-sync": [
            "rsync -KPar --delete --force --files-from=scripts/deploy.txt ./ data/artifact/"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Don't override media title with metadata on translation save.": "https://www.drupal.org/files/issues/2019-04-25/media-translation_hassourcefieldchanged-2999370-11.patch"
            },
            "drupal/inline_entity_form": {
                "Support entity revision references": "https://www.drupal.org/files/issues/2019-10-07/inline_entity_form-2367235-131.patch"
            },
            "drupal/scheduler_content_moderation_integration": {
                "Undefined offset bug.": "https://www.drupal.org/files/issues/2020-02-19/undefined_offset_node_creation-3077147-12.patch"
            }
        },
        "installer-types": [
            "library"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
