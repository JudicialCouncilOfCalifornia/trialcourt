    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 600,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/modules/ -type d -name .git | xargs rm -rf",
            "find ./web/themes/custom/ -type d -name .git | xargs rm -rf",
            "find ./web/profiles/ -type d -name .git | xargs rm -rf",
            "find ./web/libraries/ -type d -name .git | xargs rm -rf"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf",
            "find ./web/modules/ -type d -name .git | xargs rm -rf",
            "find ./web/themes/custom/ -type d -name .git | xargs rm -rf",
            "find ./web/profiles/ -type d -name .git | xargs rm -rf",
            "find ./web/libraries/ -type d -name .git | xargs rm -rf"
        ],
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff defined yet.'",
        "unit-test": "echo 'No unit test step defined.'",
        "artifact-sync": [
            "rsync -KPar --delete --force --exclude='node_modules' --files-from=scripts/deploy.txt ./ data/artifact/"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Don't override media title with metadata on translation save.": "https://www.drupal.org/files/issues/2019-04-25/media-translation_hassourcefieldchanged-2999370-11.patch",
                "Dynamically provide action plugins for content moderation": "https://www.drupal.org/files/issues/2020-01-25/2797583-149.patch",
                "Content moderation state entity query support": "https://www.drupal.org/files/issues/2020-09-25/3025164-28.patch",
                "Add Mayan and Tigrinya language support": "patches/add-mayan-tigrinya-languages-02.patch",
                "Enable Needs Translation on moderated content.": "https://www.drupal.org/files/issues/2022-02-01/2950626-14.patch",
                "Recursive rendering images when they are 20+": "https://www.drupal.org/files/issues/2021-09-24/3151977-d9-media-recursive-rendering.patch",
                "Required fields support under collapsed details grouping": "https://www.drupal.org/files/issues/2023-05-24/3333481-fix-details-html5-validation-10.patch",
                "Taxonomy terms load in admin side give white screen": "https://www.drupal.org/files/issues/2023-06-06/2183565-69.patch",                    
                "Fix for unable to decode outout into JSON": "patches/locale_patch.patch",
                "PoStreamReader error": "https://www.drupal.org/files/issues/2023-01-17/drupal_core-PoStreamReader_readLine_throws_an_error_on_module_install-3301239-7.patch",
                "Fix for PHP warning isArray check": "patches/fixphp-warning.patch"
            },
            "drupal/entity_usage": {
                "Remove old rivisions from usage count.": "https://www.drupal.org/files/issues/2020-09-26/entity_usage_views_integration_usage_count-3043388-6.patch" 
            },          
            "drupal/devel": {
                "Fix devel count() warning in php 7.2": "patches/devel-count-warning.patch",
                "Fix Cannot unset offset in a non-array variable in devel_get_handlers" : "patches/devel-unset-issue.patch"
            },
            "drupal/json_feed": {
                "Adds JSONP support to feeds": "patches/json_feed_jsonp_support.patch"
            },
            "drupal/regex_redirect": {
                "Patched module for proper string comparison": "patches/3172316-captured-value-not.patch"
            },
            "drupal/migrate_plus": {
                "Convert SimpleXMLElement to array": "https://www.drupal.org/files/issues/2022-05-31/migrate_tools-xml-class-too-3076534-19.patch"
            },
            "drupal/migrate_process_inline_images": {
                "Better local file detection": "https://www.drupal.org/files/issues/2020-01-09/migrate_process_inline_images-3105332-2_0.patch",
                "Core version requirement.": "https://www.drupal.org/files/issues/2020-11-12/core_version_requirement_issue-3182162-2.patch"
            },
            "drupal/redirect": {
                "Allowing wildcards": "https://www.drupal.org/files/issues/2020-09-21/support-for-wildcards-2831605-56.patch"
            },
            "drupal/video_embed_field": {
                "Allow creation of new video media entities using the Media Browser": "https://www.drupal.org/files/issues/2020-06-20/vef-3039873-media-library-add-32.patch"
            },
            "drupal/autocomplete_deluxe": {
                "Allow multiple references with same title": "https://www.drupal.org/files/issues/2019-07-25/3053986_4.patch",
                "undefined-autocomplete-data-key": "https://www.drupal.org/files/issues/2022-06-09/undefined-autocomplete-data-key-3284962-2.patch"
            },
            "drupal/sendgrid_integration": {
                "Sendgrid email attachments": "https://www.drupal.org/files/issues/2020-12-21/2983707-18.patch"
            },
            "drupal/migrate_google_sheets": {
                "Update to Google v4 API": "https://www.drupal.org/files/issues/2021-11-03/migrate_google_sheets-google_sheets_v4_api-3236814-3.patch",
                "Resolve incompatibility with migrate_plus": "patches/migrate-google-sheets-declaration.patch"
            },
            "drupal/token": {
                "Prevent menu link on node preview": "https://www.drupal.org/files/issues/2021-08-13/responsive_preview_support_3218969-6.patch"
            },
            "drupal/filefield_paths": {
                "Fix private file path redirects.": "https://www.drupal.org/files/issues/2022-05-26/3269636-incorrect-private-file-redirect-path-2.patch",
                "https://www.drupal.org/files/issues/2021-07-19/incorrect_redirect_hash_generated-3045063-7.patch": "https://www.drupal.org/files/issues/2021-07-19/incorrect_redirect_hash_generated-3045063-7.patch"
            },
            "drupal/advagg": {
                "Fix for stripos deprecated": "patches/advagg-stripos-fix.patch"
            },            
            "drupal/fullcalendar_view" : {
                "Fix for Calendar options": "patches/fix-jserror-fullcalendarview.patch"    
            },
            "drupal/search_api_autocomplete": {
            "Fix for Solr error":"https://www.drupal.org/files/issues/2023-10-01/3382226-22--solr_version_check_bug.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
